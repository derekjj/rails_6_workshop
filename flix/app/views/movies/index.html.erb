<ul>
<% @movies.each do |movie| %>
  <%# <li>Iron Man</li>
  <li>Superman</li>
  <li>Spider-Man</li> %>
  <li>
    <%= movie.title %>
    (<%= movie.rating %>)
    <%= number_to_currency(movie.total_gross, precision: 0) %>
    <p>
      <%= movie.description %>
    </p>
    <p>
      <%= movie.released_on %>
    </p>
  </li>
<% end %>
</ul>